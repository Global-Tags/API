{
    "port": 5000,
    "ipHeaders": ["x-real-ip"],
    "strictAuth": true,
    "logLevel": "Info", // Error, Warn, Info or Debug
    "logTimeFormat": "YYYY-MM-DD HH:mm:ss",
    "srv": "",
    "validation": {
        "tag": {
            "min": 1,
            "max": 30,
            "blacklist": [],
            "watchlist": []
        },
        "icon": {
            "blacklist": []
        }
    },
    "ratelimit": {
        "active": true,
        "routes": [
            {
                "method": "GET",
                "regex": "\/players\/\\w+\/?",
                "max": 1000,
                "seconds": 60
            },
            {
                "method": "POST",
                "regex": "^\/players\/\\w+\/?$",
                "max": 5,
                "seconds": 60
            },
            {
                "method": "POST",
                "regex": "\/players\/\\w+\/admin/?",
                "max": 2,
                "seconds": 30
            },
            {
                "method": "POST",
                "regex": "\/players\/\\w+/position\/?",
                "max": 5,
                "seconds": 60
            },
            {
                "method": "POST",
                "regex": "\/players\/\\w+/icon\/?",
                "max": 5,
                "seconds": 60
            },
            {
                "method": "POST",
                "regex": "\/players\/\\w+/report\/?",
                "max": 5,
                "seconds": 60
            }
        ]
    },
    "bot": {
        "enabled": true,
        "token": "",
        "staff": [],
        "reports": {
            "active": true,
            "channel": "",
            "content": ""
        },
        "watchlist": {
            "active": true,
            "channel": "",
            "content": ""
        },
        "appeals": {
            "active": true,
            "channel": "",
            "content": ""
        },
        "mod_log": {
            "active": true,
            "channel": ""
        },
        "connection": {
            "active": true,
            "role": "",
            "log": ""
        }
    }
}